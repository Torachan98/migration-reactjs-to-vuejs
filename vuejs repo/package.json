{
  "name": "vue-demo",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "run-s lint:*",
    "lint:oxlint": "oxlint . --fix",
    "lint:eslint": "eslint . --fix --cache",
    "format": "prettier --write --experimental-cli src/",
    "api:clean": "rimraf src/api/generated",
    "api:fetchSwagger": "cross-env-shell NODE_TLS_REJECT_UNAUTHORIZED=0 swagger2openapi https://localhost:5000/swagger/v1/swagger.json -o swagger-openapi.json",
    "api:generate": "npm run api:fetchSwagger && npx openapi-typescript-codegen --input swagger-openapi.json --output src/api/generated --client axios --useOptions --useUnionTypes"
  },
  "dependencies": {
    "@fortawesome/free-brands-svg-icons": "^7.1.0",
    "@fortawesome/free-regular-svg-icons": "^7.1.0",
    "@fortawesome/free-solid-svg-icons": "^7.1.0",
    "@fortawesome/vue-fontawesome": "^3.1.3",
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/vite": "^4.1.18",
    "@vueform/toggle": "^2.1.4",
    "autoprefixer": "^10.4.24",
    "axios": "^1.13.4",
    "cross-env": "^10.1.0",
    "firebase": "^12.9.0",
    "jwt-decode": "^4.0.0",
    "openapi-typescript-codegen": "^0.30.0",
    "pinia": "^3.0.4",
    "rimraf": "^6.1.2",
    "swagger2openapi": "^7.0.8",
    "vite-imagetools": "^9.0.2",
    "vue": "^3.5.27",
    "vue-router": "^5.0.1",
    "vue3-spinners": "^1.3.3",
    "vue3-toastify": "^0.2.8"
  },
  "devDependencies": {
    "@tsconfig/node24": "^24.0.4",
    "@types/node": "^24.10.9",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitejs/plugin-vue-jsx": "^5.1.3",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-oxlint": "~1.42.0",
    "eslint-plugin-vue": "~10.7.0",
    "jiti": "^2.6.1",
    "npm-run-all2": "^8.0.4",
    "oxlint": "~1.42.0",
    "prettier": "^3.8.1",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.9.3",
    "vite": "beta",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vue-tsc": "^3.2.4"
  },
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  }
}
