{
  "name": "identity-admin",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "api:clean": "rimraf src/api/generated",
    "api:fetchSwagger": "cross-env-shell NODE_TLS_REJECT_UNAUTHORIZED=0 swagger2openapi https://localhost:5000/swagger/v1/swagger.json -o swagger-openapi.json",
    "api:generate": "npm run api:fetchSwagger && npx openapi-typescript-codegen --input swagger-openapi.json --output src/api/generated --client axios --useOptions --useUnionTypes"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^7.1.0",
    "@fortawesome/free-brands-svg-icons": "^7.1.0",
    "@fortawesome/free-regular-svg-icons": "^7.1.0",
    "@fortawesome/free-solid-svg-icons": "^7.1.0",
    "@fortawesome/react-fontawesome": "^3.1.1",
    "@reduxjs/toolkit": "^2.2.0",
    "@vitejs/plugin-react": "^5.1.2",
    "axios": "^1.6.0",
    "firebase": "^12.8.0",
    "jwt-decode": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.0",
    "react-loader-spinner": "^8.0.0",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.0",
    "react-switch": "^7.1.0",
    "react-toastify": "^11.0.5",
    "vite-imagetools": "^9.0.2"
  },
  "devDependencies": {
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "autoprefixer": "^10.4.0",
    "cross-env": "^10.1.0",
    "openapi-typescript-codegen": "^0.30.0",
    "postcss": "^8.4.0",
    "rimraf": "^6.1.2",
    "swagger2openapi": "^7.0.8",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.2.0",
    "vite": "^5.0.0"
  }
}
